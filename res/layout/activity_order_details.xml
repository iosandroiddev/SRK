                    android:layout_weight="0.5"
                    android:gravity="center_vertical"
                    android:orientation="vertical" >

                    <com.utils.CustomTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="3dp"
                        android:text="@string/cost"
                        android:textColor="@color/black"
                        android:textSize="13sp"
                        custom:fonttype="trebuchet_ms" />
                </LinearLayout>

                <com.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:padding="3dp"
                    android:text="@string/_"
                    android:textColor="@color/black"
                    android:textSize="13sp"
                    android:visibility="invisible"
                    custom:fonttype="trebuchet_ms" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:gravity="center_vertical"
                    android:orientation="vertical" >

                    <com.utils.CustomTextView
                        android:id="@+id/txtLogisticsTaxCost"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:padding="3dp"
                        android:textColor="@color/black"
                        android:textSize="13sp"
                        custom:fonttype="trebuchet_ms" />
                </LinearLayout>
            </LinearLayout>

            <com.utils.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:background="#CAE1FF"
                android:gravity="center_vertical"
                android:padding="3dp"
                android:text="@string/the_product_rental_process_will_start_only_after_we_verify_your_details_thanks_for_helping_us_build_a_trusted_platform_"
                android:textColor="@color/black"
                android:textSize="13sp"
                custom:fonttype="trebuchet_ms" />

            <com.utils.CustomTextView
                android:id="@+id/btnContinue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_margin="10dp"
                android:background="@drawable/blue_custom_button_selector"
                android:gravity="center"
                android:marqueeRepeatLimit="marquee_forever"
                android:padding="10dp"
                android:singleLine="true"
                android:text="@string/btn_continue"
                android:textColor="@color/white"
                android:textSize="16sp"
                custom:fonttype="trebuchet_ms" />
        </LinearLayout>
    </ScrollView>

</FrameLayout>